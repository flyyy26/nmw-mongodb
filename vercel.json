{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "public/$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000"
        }
      }
    ],
    "fs": {
      "public": {
        "readOnly": false
      }
    }
  }